{
    "name": "marvinrabe/blade-flags",
    "description": "A package to easily make use of country flags in your Laravel Blade views.",
    "keywords": ["Blade", "Flags", "Laravel"],
    "homepage": "https://github.com/marvinrabe/blade-flags",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Rabe",
            "homepage": "https://marvinrabe.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MarvinRabe\\BladeFlags\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarvinRabe\\BladeFlags\\BladeFlagsServiceProvider"
            ]
        }
    },
    "scripts": {
        "generate": "blade-icons-generate"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
